{
  "name": "@whitecloak/nestjs-passport-firebase",
  "version": "1.0.7+1",
  "description": "Passport - Firebase Auth Module for NestJS",
  "author": "White Cloak Technologies, Inc.",
  "license": "MIT",
  "main": "dist/index.js",
  "files": [
    "dist/**/*",
    "*.md"
  ],
  "scripts": {
    "start:dev": "tsc -w",
    "build": "tsc",
    "prepare": "npm run build",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "tslint -p tsconfig.json -c tslint.json",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "keywords": [
    "nestjs",
    "passport",
    "firebase",
    "firebase-auth"
  ],
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/whitecloakph/nestjs-passport-firebase.git"
  },
  "peerDependencies": {
    "@nestjs/common": "^8.0.0 || ^9.0.0 || ^10.0.0",
    "@nestjs/passport": "^9.0.0 || ^10.0.0",
    "jwks-rsa": "^2.0.5 || ^3.0.0",
    "passport": "^0.6.0 || ^0.7.0",
    "passport-jwt": "^4.0.0",
    "firebase-admin": "^12.4.0",
    "passport-custom": "^1.1.1"
  },
  "optionalDependencies": {
    "passport-cookie": "^1.0.9"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.4.5",
    "@nestjs/common": "^10.4.1",
    "@nestjs/core": "^10.4.1",
    "@nestjs/passport": "^10.0.3",
    "@nestjs/platform-express": "^10.4.1",
    "@nestjs/schematics": "10.1.4",
    "@nestjs/testing": "^10.4.1",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/node": "^22.5.2",
    "@types/passport-jwt": "^4.0.1",
    "@types/supertest": "^6.0.2",
    "@typescript-eslint/eslint-plugin": "^8.4.0",
    "@typescript-eslint/parser": "^8.4.0",
    "eslint": "^9.9.1",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "firebase-admin": "^12.4.0",
    "jest": "^29.7.0",
    "jwks-rsa": "^3.1.0",
    "passport": "^0.7.0",
    "passport-custom": "^1.1.1",
    "passport-jwt": "^4.0.1",
    "prettier": "^3.3.3",
    "reflect-metadata": "^0.2.2",
    "rimraf": "^6.0.1",
    "rxjs": "^7.8.1",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.5",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.5.4"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "bugs": {
    "url": "https://github.com/whitecloakph/nestjs-passport-firebase/issues"
  },
  "homepage": "https://github.com/whitecloakph/nestjs-passport-firebase#readme"
}
